extends layout

block content
  div.main
    each val, index in project
      div(style= "background-image:url(./" + val.imgPath[1] + "), linear-gradient(150deg, rgb(204, 204, 204), rgba(0, 0, 0, 0) 50%); ").project
        a(href='/project' + val.url )
          div.textProject
            h2 #{val.title}
        
